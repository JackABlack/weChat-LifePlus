  text-align: center;
    font-size: 50px;
  color: white;
display: block;
    margin-top: 10rpx;
  z-index: -1;
  animation: fadein 0.5s ease;
  font-family: '华文细黑';
}

@keyframes fadein {
  0% {
    opacity: 0;
    font-size: 0;
  }

  15% {
    opacity: 0.15;
    font-size: 36;
  }

  25% {
    opacity: 0.45;
    font-size: 24;
  }

  50% {
    opacity: 0.65;
    font-size: 30;
  }

  100% {
    opacity: 1;
    font-size: 20;
  }
}

